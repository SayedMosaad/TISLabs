@model IEnumerable<ContactUs>
@{
    ViewData["Title"] = "Contact Us";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="card">
    <div class="card-header">
        <h6 class="card-title">Contact Us Data </h6>
    </div>
    <div class="card-body">
        <table class="table datatable table-hover table-striped">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Mobile</th>
                    <th>Email</th>
                    <th>Message</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Name</td>
                        <td>@item.Mobile</td>
                        <td>@item.Email</td>
                        <td>@item.MessageBody</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
